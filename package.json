{
  "name": "@iolave/bun-template",
  "version": "v0.0.0-beta.1",
  "main": "main.ts",
  "module": "src/main.ts",
  "devDependencies": {
    "bun-types": "latest",
    "tsc-watch": "^6.0.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "start": "bun lint && bun run ./src/main.ts",
